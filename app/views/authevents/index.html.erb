<table>
  <thead>
    <tr>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @authevents.each do |authevent| %>
      <tr>
        <td><%= link_to 'Show', authevent %></td>
